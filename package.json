{
    "name": "namma-ui-framework",
    "version": "1.0.1",
    "private": true,
    "engines": {
        "node": ">=16.x"
    },
    "scripts": {
        "dev": "env-cmd -f ./env/.env.development next dev -p 4200",
        "debug": "env-cmd -f ./env/.env.development NODE_OPTIONS='--inspect' next dev -p 4200",
        "build": "next build",
        "start": "next start -p 5000",
        "build:qa": "env-cmd -f ./env/.env.qa next build",
        "build:staging": "env-cmd -f ./env/.env.staging next build",
        "build:production": "env-cmd -f ./env/.env.production next build",
        "test:watch": "jest --watch",
        "test": "NODE_ENV=test jest",
        "storybook": "start-storybook -p 6006",
        "build:storybook": "build-storybook"
    },
    "author": "Yashjeet Luthra <yash@napses.com>, Priyansh Jain <priyansh.jain@napses.com>",
    "license": "Napses",
    "dependencies": {
        "@cloudflare/stream-react": "^1.8.0",
        "axios": "^0.27.2",
        "chart.js": "^4.2.0",
        "classnames": "^2.3.2",
        "draft-js": "^0.11.7",
        "env-cmd": "^10.1.0",
        "eslint": "8.26.0",
        "eslint-config-next": "13.0.1",
        "flagsmith": "^3.3.2",
        "icomoon-react": "^3.0.0",
        "install": "^0.13.0",
        "next": "^12.2.2",
        "react": "^18.2.0",
        "react-chartjs-2": "^5.2.0",
        "react-datepicker": "^4.10.0",
        "react-dom": "^18.2.0",
        "react-draft-wysiwyg": "^1.15.0",
        "react-infinite-scroll-component": "^6.1.0",
        "react-loading-skeleton": "^3.1.0",
        "react-player": "^2.11.2",
        "react-range": "^1.8.14",
        "react-table": "^7.8.0",
        "react-tailwindcss-select": "^1.6.0",
        "sinon": "^14.0.0",
        "tailwindcss-rtl": "^0.9.0"
    },
    "devDependencies": {
        "@babel/core": "^7.18.6",
        "@babel/eslint-parser": "^7.18.2",
        "@storybook/addon-actions": "^6.5.16",
        "@storybook/addon-essentials": "^6.5.16",
        "@storybook/addon-interactions": "^6.5.16",
        "@storybook/addon-links": "^6.5.16",
        "@storybook/addon-postcss": "^2.0.0",
        "@storybook/builder-webpack5": "^6.5.16",
        "@storybook/manager-webpack5": "^6.5.16",
        "@storybook/react": "^6.5.16",
        "@storybook/testing-library": "0.0.13",
        "@testing-library/dom": "^8.16.0",
        "@testing-library/jest-dom": "^5.16.4",
        "@testing-library/react": "^13.3.0",
        "autoprefixer": "^10.4.13",
        "babel-jest": "^28.1.2",
        "babel-loader": "^8.3.0",
        "eslint": "^8.19.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-mocha": "^10.0.5",
        "eslint-plugin-react-hooks": "^4.6.0",
        "jest": "^28.1.2",
        "jest-fetch-mock": "^3.0.3",
        "postcss": "^8.4.21",
        "postcss-loader": "^7.0.2",
        "tailwindcss": "^3.2.7"
    },
    "jest": {
        "testPathIgnorePatterns": [
            "<rootDir>/.next/",
            "<rootDir>/node_modules/"
        ],
        "setupFilesAfterEnv": [
            "<rootDir>/setupTests.js"
        ],
        "transform": {
            "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest"
        },
        "moduleNameMapper": {
            "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
            "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
        }
    }
}
